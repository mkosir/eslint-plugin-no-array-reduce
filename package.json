{
  "name": "eslint-plugin-no-array-reduce",
  "version": "0.0.0-development",
  "description": "ESLint rule to disallow Array.reduce() method.",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "tsup",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc --noEmit -p tsconfig.dev.json",
    "format-lint": "prettier --config .prettierrc -l --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown .",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main",
    "validate": "npm run lint && npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/eslint": "^8.4.1",
    "@types/estree": "0.0.51",
    "@types/node": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^19.0.2",
    "tsup": "^5.12.6",
    "tsc-files": "^1.1.3",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "eslint": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkosir/eslint-plugin-no-array-reduce.git"
  },
  "author": "Marko Kosir <kosir.mar@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mkosir/eslint-plugin-no-array-reduce/issues"
  },
  "homepage": "https://github.com/mkosir/eslint-plugin-no-array-reduce#readme",
  "keywords": [
    "javascript",
    "typescript",
    "eslint",
    "eslint-plugin",
    "eslint-rules",
    "reduce"
  ]
}
